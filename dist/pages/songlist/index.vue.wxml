        <van-button wx:if="{{isEdit}}" size="large" class="_van-button data-v-86a5f636 btn" type="warning">马上播放</van-button>
        <van-button wx:if="{{isEdit}}" size="large" class="_van-button data-v-86a5f636 btn" bindtap="handleProxy" type="danger" data-eventid="{{'4'}}" data-comkey="{{$k}}">移除歌单</van-button>
      </view>
    </view>
  </view>
</template>